{
    "name": "desmart/php-enum",
    "type": "library",
    "description": "Strongly-typed enum objects for PHP",
    "license": "MIT",
    "keywords": ["desmart", "php", "enum"],
    "authors": [
        {
            "name": "Micha≈Ç Golon",
            "email": "gollum@desmart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DeSmart\\Enum\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DeSmart\\Enum\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
